{
  "author": "https://bmaupin.github.io",
  "bugs": {
    "url": "https://github.com/bmaupin/js-civ5save/issues"
  },
  "dependencies": {
    "babel-runtime": "^6.26.0"
  },
  "description": "Library for working with Civilization V save files",
  "devDependencies": {
    "babel-eslint": "^7.2.3",
    "babel-jest": "^20.0.3",
    "babel-loader": "^7.1.2",
    "babel-minify-webpack-plugin": "^0.2.0",
    "babel-plugin-transform-builtin-extend": "^1.1.2",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-env": "^1.6.0",
    "coveralls": "^2.13.1",
    "esdoc": "^1.0.2",
    "esdoc-standard-plugin": "^1.0.0",
    "eslint": "^4.2.0",
    "eslint-loader": "^1.9.0",
    "jest": "^20.0.4",
    "webpack": "^3.3.0"
  },
  "esdoc": {
    "source": "./src",
    "destination": "./docs",
    "plugins": [
      {
        "name": "esdoc-standard-plugin",
        "option": {
          "accessor": {
            "access": [
              "public"
            ],
            "autoPrivate": true
          },
          "undocumentIdentifier": {
            "enable": false
          }
        }
      }
    ]
  },
  "files": [
    "dist/"
  ],
  "homepage": "https://github.com/bmaupin/js-civ5save#readme",
  "jest": {
    "testURL": "file://test"
  },
  "keywords": [
    "civ5save",
    "civ5",
    "CivV",
    "Civilization 5",
    "Civilization V"
  ],
  "license": "MIT",
  "main": "dist/civ5save.js",
  "name": "civ5save",
  "repository": {
    "type": "git",
    "url": "https://github.com/bmaupin/js-civ5save.git"
  },
  "scripts": {
    "build": "webpack && NODE_ENV=production webpack && esdoc",
    "lint": "eslint src/* --ext .js --ext .json",
    "test": "jest --coverage"
  },
  "version": "0.2.0"
}
